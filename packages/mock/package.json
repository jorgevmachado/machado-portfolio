{
  "name": "@repo/mock",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "./dist/**"
  ],
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "scripts": {
    "gen": "turbo gen business",
    "watch": "nodemon --exec ts-node src/app.ts",
    "build": "rimraf dist && tsc -b -v",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --max-warnings 0 --fix"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*/index.d.ts",
      "import": "./dist/*.js",
      "require": "./dist/*.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "src/*"
      ]
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@repo/business": "*",
    "@repo/eslint-config": "*",
    "@repo/services": "*",
    "@repo/typescript": "*",
    "@types/json-server": "^0.14.7",
    "@types/node": "^20.3.1",
    "json-server": "^0.17.4",
    "nodemon": "^3.1.9",
    "rimraf": "^6.0.1",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "typescript": "5.5.4"
  }
}
